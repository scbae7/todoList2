.modal#myPageModal
    div(class="modalContent myPageModalCont")
        .myPageTitle
            span.myIcon
            span.myPageLabel MY PAGE
        .myPageSection
            .myContInfo
                .myLabel 이름
                if(userName)
                    .myDesc=userName
                else
                    .myDesc 홍길동    
            .myEdit
                button.myEditBtn
            .myContInfo
                .myLabel E-MAIL
                if(useEmail)
                    .myDesc=userEmail
                else
                    .myDesc abc@gmail.com    
            .myEdit
                button.myEditBtn
            button.myBtn 비밀번호 변경
            button.myBtn 회원 탈퇴