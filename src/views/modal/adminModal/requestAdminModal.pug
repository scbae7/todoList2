.modal#requestModal
  div(class="modalContent requestContent")
    .requestModalTitle 관리자 승급 요청
    .requestRowCont 
      each request in requests
        if(requests)
          .requestRow(data-userId=`${request.userId}`)
            .requestName=`id = ${request.userId}`
            .requestBtnCont
              button.adminAcceptBtn.okBtn 수락 
              button.adminRejectBtn.noBtn 거절
        else
          .requestRow
            .requestName 없음
            .requestBtnCont
              button 수락 
              button 거절
